{
  "name": "escuela-rds",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "firebase deploy --only hosting:classroom-rds",
    "clean":"rm -rf ./node_modules yarn.lock package-lock.json",
    "doc:g": "npx @compodoc/compodoc -p ./tsconfig.json --output ./docs --name \"Sistema RDS\" --customFavicon ./projects/classroom-rds/src/assets/icons/favicon-196.png --customLogo ./projects/classroom-rds/src/assets/images/rds-newlogo-transparent.png --theme material --hideGenerator --language es-ES",
    "pwa:assets-std-logo": "npx pwa-asset-generator ./projects/classroom-rds/src/assets/images/rds-newlogo-transparent.png ./projects/classroom-rds/src/assets/icons/ --manifest ./projects/classroom-rds/src/manifest.webmanifest --index ./projects/classroom-rds/src/index.html --type png --maskable true --opaque false --mstile --favicon",
    "pwa:assets-icons": "npx pwa-asset-generator ./projects/classroom-rds/src/assets/images/rds-newlogo-transparent.png ./projects/classroom-rds/src/assets/icons/ --only-icons --index ./projects/classroom-rds/src/index.html --favicon"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.9",
    "@angular/cdk": "^12.2.9",
    "@angular/common": "~12.2.9",
    "@angular/compiler": "~12.2.9",
    "@angular/core": "~12.2.9",
    "@angular/fire": "^7.1.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~12.2.9",
    "@angular/material": "^12.2.9",
    "@angular/platform-browser": "~12.2.9",
    "@angular/platform-browser-dynamic": "~12.2.9",
    "@angular/pwa": "^12.1.1",
    "@angular/router": "~12.2.9",
    "@angular/service-worker": "~12.2.9",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ngrx/data": "^12.4.0",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/router-store": "^12.4.0",
